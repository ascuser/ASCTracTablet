<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ASCTracTablet.Views.Maintenances.pageOverride">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lblPrompt" Text="Enter Override" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="{StaticResource colorASCBlue}" TextColor="Black" FontSize="24" />
            <Entry x:Name="edOverride"  VerticalOptions="Center" HorizontalOptions="FillAndExpand" FontSize="24" Completed="OnOverride"/>
            <Button x:Name="btnOverride" Text="Continue" Clicked="OnOverride" VerticalOptions="Center" HorizontalOptions="Center" FontSize="24" BackgroundColor="#EEB422"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
